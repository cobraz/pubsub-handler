{
  "name": "pubsub-http-handler",
  "version": "3.0.1",
  "description": "Handle Google PubSub requests with ease",
  "repository": "github:cobraz/pubsub-http-handler",
  "license": "Apache-2.0",
  "author": "Simen A. W. Olsen <cobraz@cobraz.no>",
  "main": "dist/index.js",
  "engines": {
    "node": ">=14.15"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint 'src/**/*.ts' --fix",
    "generate:docs": "typedoc --mode file --readme none --plugin typedoc-plugin-markdown --hideSources --out docs src",
    "test": "jest --coverage src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "keywords": [
    "google",
    "app",
    "cloud",
    "cloud-run",
    "run",
    "pubsub",
    "handler",
    "fastify",
    "fastify-plugin",
    "cloud-functions"
  ],
  "dependencies": {
    "@sinclair/typebox": "^0.23.0",
    "@types/express": "^4.17.13",
    "fastify": "^3.9.2",
    "fastify-plugin": "^3.0.0"
  },
  "devDependencies": {
    "@tsconfig/node14": "^1.0.1",
    "@types/jest": "^26.0.15",
    "@types/json-schema-faker": "^0.5.1",
    "@types/node": "^14.14.16",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/parser": "4.30.0",
    "eslint": "7.14.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.24.2",
    "husky": "^4.3.0",
    "jest": "26.6.3",
    "json-schema-faker": "^0.5.0-rcv.32",
    "lint-staged": "11.1.2",
    "prettier": "2.5.0",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.19.2",
    "typedoc-plugin-markdown": "^3.1.1",
    "typescript": "4.1.2"
  }
}
