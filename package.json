{
  "name": "pubsub-http-handler",
  "version": "4.0.0",
  "description": "Handle Google PubSub requests with ease",
  "repository": "github:cobraz/pubsub-http-handler",
  "license": "Apache-2.0",
  "author": "Simen A. W. Olsen <cobraz@cobraz.no>",
  "main": "dist/index.js",
  "engines": {
    "node": ">=16"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint '**/*.ts' --fix",
    "generate:docs": "typedoc --mode file --readme none --plugin typedoc-plugin-markdown --hideSources --out docs src",
    "test": "jest --coverage src"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "prettier --write"
    ]
  },
  "keywords": [
    "google",
    "app",
    "cloud",
    "cloud-run",
    "run",
    "pubsub",
    "handler",
    "fastify",
    "fastify-plugin",
    "cloud-functions"
  ],
  "dependencies": {
    "@fastify/type-provider-typebox": "^1.0.0",
    "@sinclair/typebox": "^0.23.5",
    "@types/express": "^4.17.13",
    "fastify": "^4.0.1",
    "pino": "^8.0.0"
  },
  "prettier": "@cobraz/prettier",
  "devDependencies": {
    "@bjerk/eslint-config": "^2.0.1",
    "@cobraz/prettier": "^2.0.0",
    "@tsconfig/node16": "^1.0.3",
    "@types/jest": "^26.0.15",
    "@types/json-schema-faker": "^0.5.1",
    "@types/node": "^16",
    "husky": "^4.3.0",
    "jest": "26.6.3",
    "json-schema-faker": "^0.5.0-rcv.32",
    "lint-staged": "11.1.2",
    "prettier": "^2.6.2",
    "ts-jest": "^26.4.4",
    "typedoc": "^0.19.2",
    "typedoc-plugin-markdown": "^3.1.1",
    "typescript": "^4.3"
  }
}
